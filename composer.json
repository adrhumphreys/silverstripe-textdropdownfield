{
    "name": "adrhumphreys/silverstripe-textdropdownfield",
    "description": "A text field combined with a dropdown field.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Field"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AdrHumphreys\\TextDropdownField\\": "src/",
            "AdrHumphreys\\TextDropdownField\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
